  <plugin>
    org.contikios.cooja.plugins.ScriptRunner
    <plugin_config>
      <script>GENERATE_MSG(2000, 'init-6lbr');
GENERATE_MSG(2000, 'init-6ln');


lostMsgs = 0;
seenMsgs = 0;

//sim.setSpeedLimit(1.0);
TIMEOUT(2000000, if(seenMsgs &gt; 15) { log.testOK(); } );

lastMsg = -1;
packets = "_________";
hops = 0;

while(true) {
    YIELD();
 if(msg.equals("init-6lbr")) {
         log.log('Initializing LBR\n');
         write(sim.getMoteWithID(1), "JOIN_NETWORK 6LBR 1 0\n");
    }
    else if(msg.equals("init-6lr")) {
         log.log('Initializing LNs\n');
         write(sim.getMoteWithID(1), "JOIN_NETWORK 6LR 1 0\n");
    }
    else if(msg.equals("init-6ln")) {
         log.log('Initializing LNs\n');
         write(sim.getMoteWithID(2), "JOIN_NETWORK 6LN 1 0\n");
    } 
    else if(msg.startsWith("Sending")) {
        hops = 0;
    } else if(msg.startsWith("#L") &amp;&amp; msg.endsWith("1; red")) {
        hops++;
    } else if(msg.startsWith("Data")) {
        data = msg.split(" ");
        num = parseInt(data[14]);
        seenMsgs++;
        if(lastMsg != -1) {
          if(num != lastMsg + 1) {
            numMissed = num - lastMsg - 1;
            lostMsgs += numMissed;           
            log.log("Missed messages " + numMissed + " before " + num + "\n");            
            for(i = 0; i &lt; numMissed; i++) {
                packets = packets.substr(0, lastMsg + i + 1).concat("_");    
            }
          }    
        }
        packets = packets.substr(0, num).concat("*");
        log.log("" + hops + " " + packets + "\n");
        lastMsg = num;
    }
}</script>
